{
  "name": "node-ts-boilerplate",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test": "jest --passWithNoTests  --runInBand --no-cache  --silent",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:staged": "npm test -- --findRelatedTests",
    "test:coverage": "npm test -- --coverage",
    "test:unit": "npm test --watch",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "lint": "eslint .",
    "lint:fix": "npm lint -- --fix",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc -p tsconfig-build.json",
    "tsc-alias": "npx tsc-alias",
    "dev:debug": "nodemon -L --inspect=0.0.0.0:9228 src/main/server.ts",
    "dev": "nodemon -L src/main/server.ts",
    "docker": "docker-compose up -d node_ts_terminal --no-recreate && docker exec -it node_ts_terminal /bin/sh",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js -f src/ormconfig.ts -c crud_node"
  },
  "keywords": [
    "node-ts-boilerplate"
  ],
  "author": "paulo.hsilvavieira@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@swc/core": "^1.3.38",
    "@swc/helpers": "^0.4.14",
    "@swc/wasm": "^1.3.38",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.6",
    "@types/jest": "^26.0.23",
    "@types/node": "^17.0.8",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "eslint": "^7.29.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "jest": "^27.0.6",
    "jest-mock-extended": "^2.0.1",
    "regenerator-runtime": "^0.13.11",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "4.3.5"
  },
  "dependencies": {
    "express": "^4.18.2",
    "faker": "^5.5.3",
    "mjml": "^4.10.4",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.22",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "twilio": "^3.73.0",
    "typeorm": "^0.2.37",
    "typeorm-extension": "^0.2.9",
    "uuid": "^8.3.2",
    "validator": "^13.6.0"
  }
}
